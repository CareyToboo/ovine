{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./lib/.tsbuildinfo",
    "target": "es5",
    "module": "esnext",
    "lib": ["es6", "dom"],
    "incremental": true,
    "jsx": "react",
    "rootDir": "src",
    "outDir": "lib",
    "baseUrl": "./src/",
    "noImplicitReturns": false,
    "typeRoots": ["typings"],
    "paths": {
      "@/*": ["./*"]
    }
  }
}
