{
  "name": "rt-admin",
  "version": "0.0.1",
  "description": "An excellent admin system.",
  "private": true,
  "scripts": {
    "dll": "webpack -p --progress --config build/webpack.dll.conf.js",
    "dev": "cross-env PORT=7050 API_ENV=localhost webpack-dev-server --config build/webpack.dev.conf.js",
    "build": "cross-env PORT=7050 API_ENV=production ENV=production webpack --config build/webpack.prod.conf.js",
    "build:all": "yarn dll && yarn build",
    "build:analyzer": "cross-env API_ENV=production ENV=production ENABLE_ANALYZER=true webpack --config build/webpack.prod.conf.js",
    "lint:ts": "tslint src/**/*.{ts,tsx}",
    "lint:ts:fix": "yarn run lint:ts --fix",
    "lint-staged": "lint-staged",
    "pretty": "prettier --loglevel warn --write \"*.{js,md,yml,json,yaml}\" \"docs/**/*.md\" \"src/**/*.{css}\" ",
    "clean": "rimraf ./dist/*"
  },
  "keywords": [
    "admin",
    "react",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "hash.js": "^1.1.7",
    "immer": "^3.1.1",
    "react": "^16.8.6",
    "react-router-config": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "rt-admin-lib": "^0.0.1",
    "styled-components": "^4.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.8.15",
    "@types/styled-components": "^4.1.14",
    "assets-webpack-plugin": "^3.9.10",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.1.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.17.0",
    "react-hot-loader": "^4.8.4",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.4.5",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react-hooks": "^2.1.0",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "lint-staged": {
    "*.{js,json,md,yml,yaml}|docs/**/*.md|src/**/*.{css}": "yarn pretty",
    "src/**/*.{ts,tsx}": "yarn lint:ts:fix && yarn lint:style:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged && git add --all"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
